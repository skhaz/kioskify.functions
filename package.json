{
  "main": "dist/index.js",
  "private": true,
  "devDependencies": {
    "firebase-tools": "6.11.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "rimraf": "2.6.3"
  },
  "dependencies": {
    "@google-cloud/pubsub": "0.29.1",
    "firebase-admin": "8.0.0",
    "firebase-functions": "2.3.1",
    "request": "2.88.0",
    "ytdl-core": "0.29.2"
  },
  "engines": {
    "node": "8"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && parcel build src/index.js --target node --detailed-report",
    "deploy": "yarn build && firebase deploy --force --only functions",
    "format": "prettier --write src/**/*.js"
  }
}
